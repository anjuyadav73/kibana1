{
  "name": "eslint-plugin-ban",
  "version": "1.5.2",
  "description": "Allows you to bannish some methods or functions.",
  "repository": {
    "type": "git",
    "url": "https://github.com/remithomas/eslint-plugin-ban"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "ban",
    "function",
    "method"
  ],
  "author": "RÃ©mi THOMAS",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write 'lib/*.js' 'tests/**/*.js'",
    "test": "mocha tests --recursive",
    "test:watch": "mocha tests --recursive --watch",
    "validate": "npm run lint && npm run test"
  },
  "dependencies": {
    "requireindex": "~1.2.0"
  },
  "devDependencies": {
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "growl": "^1.10.5",
    "husky": "^4.3.6",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run validate"
    }
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "ISC"
}
